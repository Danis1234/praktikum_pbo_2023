package tugas55pbo;

public class Main {
    public static void main(String[] args) {
        Android androidPhone = new Android("Samsung", "Android", "Grand", 3000000, "234ibfd3840fo");
        androidPhone.displayProduct();
        System.out.println("Android Key Store: " + androidPhone.getKeyStore());

        BlackBerry blackberryPhone = new BlackBerry("BlackB", "RIM", "Curve", 2000000, "BHS249");
        blackberryPhone.displayProduct();
        System.out.println("PIN: " + blackberryPhone.getPinBB());

        WindowsPhone windowsPhone = new WindowsPhone("Nokia", "WinB", "Lumia", 1000000, "UUUQIJWORJ");
        windowsPhone.displayProduct();
        System.out.println("Wp Key Store: " + windowsPhone.getWpKeyStore());
    }
}
